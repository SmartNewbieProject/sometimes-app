        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f6df6d86-2504-4574-8bf2-e069c6e76316"
      },
      "enableRemoteLogging": true
