{
  "cli": {
    "version": ">= 16.4.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "ios": {
        "distribution": "store",
        "developmentClient": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "distribution": "store",
        "developmentClient": false,
        "buildType": "apk"
      }
    },
    "production-debug": {
      "extends": "production",
      "ios": {
        "buildConfiguration": "Debug"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
